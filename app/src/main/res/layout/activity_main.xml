<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <FrameLayout
        android:id="@+id/framelayout"
        android:layout_width="392dp"
        android:layout_height="733dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>
    <FrameLayout
        android:id="@+id/framelayout_calendar"
        android:layout_width="392dp"
        android:layout_height="733dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="391dp"
        android:layout_height="77dp"
        android:layout_marginTop="729dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.979"
        tools:ignore="VisualLintBounds">

        <ImageView
            android:id="@+id/calendar"
            android:layout_width="130dp"
            android:layout_height="match_parent"
            android:layout_marginEnd="20dp"
            android:layout_weight="1"
            android:contentDescription="@string/calendar"
            android:visibility="visible"
            app:srcCompat="@drawable/android_contact_icon_icons_1"
            tools:srcCompat="@drawable/android_contact_icon_icons_1"
            tools:visibility="visible" />

        <ImageView
            android:id="@+id/menu"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_weight="1"
            android:contentDescription="@string/menu"
            android:visibility="visible"
            app:srcCompat="@drawable/radiant_game_android_6960"
            tools:srcCompat="@drawable/radiant_game_android_6960" />

        <ImageView
            android:id="@+id/profile"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:contentDescription="@string/profile"
            android:visibility="visible"
            app:srcCompat="@drawable/android_contact_icon_icons_com_50531"
            tools:srcCompat="@drawable/android_contact_icon_icons_com_50531" />
    </LinearLayout>






</androidx.constraintlayout.widget.ConstraintLayout>